{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "v2": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/v2",
      "sourceRoot": "projects/v2/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/v2",
            "index": "projects/v2/src/index.html",
            "main": "projects/v2/src/main.ts",
            "polyfills": "projects/v2/src/polyfills.ts",
            "tsConfig": "projects/v2/tsconfig.app.json",
            "assets": [
              "projects/v2/src/favicon.ico",
              "projects/v2/src/assets",
              "projects/v2/src/_redirects"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "projects/v2/src/styles.scss",
              "./node_modules/jexcel/dist/jexcel.css"
            ],
            "scripts": [
              "node_modules/marked/lib/marked.js"

            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/v2/src/environments/environment.ts",
                  "with": "projects/v2/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "projects/v2/src/environments/environment.ts",
                  "with": "projects/v2/src/environments/environment.stage.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "v2:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "v2:build:production"
            },
            "staging": {
              "browserTarget": "v2:build:staging"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "v2:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/v2/src/test.ts",
            "polyfills": "projects/v2/src/polyfills.ts",
            "tsConfig": "projects/v2/tsconfig.spec.json",
            "karmaConfig": "projects/v2/karma.conf.js",
            "assets": [
              "projects/v2/src/favicon.ico",
              "projects/v2/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "projects/v2/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/v2/tsconfig.app.json",
              "projects/v2/tsconfig.spec.json",
              "projects/v2/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/v2/e2e/protractor.conf.js",
            "devServerTarget": "v2:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "v2:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "v2"
}